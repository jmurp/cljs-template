{:deps {org.clojure/clojure {:mvn/version "1.9.0"}
        org.clojure/clojurescript {:mvn/version "1.10.339"}
        com.bhauman/figwheel-main {:mvn/version "0.1.9"}
        com.bhauman/rebel-readline-cljs {:mvn/version "0.1.4"}
        ring/ring-core {:mvn/version "1.7.1"}}
 :aliases {:dev {:main-opts ["-i" "scripts/server.clj" "-m" "figwheel.main" "-b" "dev" "-r"]}
           :prod-test {:main-opts ["-i" "scripts/server.clj" "-m" "figwheel.main" "-O" "advanced" "-b" "dev"]}
           :prod {:main-opts ["-i" "scripts/server.clj" "-m" "figwheel.main" "-O" "advanced" "-bo" "dev"]}}
 :paths ["src" "resources" "target"]}
